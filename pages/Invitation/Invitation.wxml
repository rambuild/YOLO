<!--pages/Invitation/Invitation.wxml-->
<view class="box">
  <view class="title"><text>分享有礼</text></view>
  <view>
    <view class="moneyBox">
      <view class="moneyBox_one one"><text>当前余额</text><text>全部提现</text></view>
      <view class="moneyBox_one tow"><text>242260.05</text><text bindtap="ShareGo">我的分享码 ></text></view>
      <view class="moneyBox_one three"><text>已提佣金：￥6300.53</text><text>累计佣金：￥10600.53</text></view>
    </view>
    <view class="money_list">
      <view class="{{index==1?'active':''}}" data-index="1" bindtap="pushMoney">提成明细</view>
      <view class="{{index==2?'active':''}}" data-index="2" bindtap="getMoney">提现明细</view>
    </view>
    <block wx:if="{{index==1}}">
      <view class="pushlist">
        <view class="pushlist_one">
          <view>提成明细</view>
          <view>2020-10-23 18:23:46</view>
        </view>
        <view>+ 163.52</view>
      </view>
    </block>
    <block wx:if="{{index==2}}">
      <view class="getlist color">
        <view class="getlist_one">
          <view>提现</view>
          <view>2020-10-23 18:23:46</view>
        </view>
        <view class="getlist_tow">审核中</view>
        <view class="getlist_therr">
          <view>-1000</view>
          <view>242260.05</view>
        </view>
      </view>
      <view class="getlist ">
        <view class="getlist_one">
          <view>提现</view>
          <view>2020-10-23 18:23:46</view>
        </view>
        <view class="getlist_tow">已提现</view>
        <view class="getlist_therr">
          <view>-1000</view>
          <view>242260.05</view>
        </view>
      </view>
    </block>

  </view>
</view>